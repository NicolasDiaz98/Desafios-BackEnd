<div>
    <h1>Bienvenido!!<h1>

    <h2>Estos son los productos agregados hasta el momento:</h2>

    {{#each products}}
    <div>
        <p>{{this.title}}</p>
        <p>{{this.description}}</p>
        <p>{{this.price}}</p>
        <p>{{this.thumbnail}}</p>
        <p>{{this.code}}</p>
        <p>{{this.stock}}</p>
        <p>{{this.id}}</p>
        <p>{{this.status}}</p>
    </div>
    {{/each}}
</div>